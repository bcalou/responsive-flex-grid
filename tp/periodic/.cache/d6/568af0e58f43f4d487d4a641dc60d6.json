n\n* {\n    box-sizing: border-box;\n}\n\n@mixin desktop {\n    @media (min-width: 1400px) {\n        @content;\n    }\n}\n\n$types: 'alkaliMetal',\n'lanthanide',\n'nobleGas',\n'transitionMetal',\n'postTransitionMetal',\n'alkalineEarthMetal',\n'actinide',\n'metalloid',\n'otherNonMetal';\n\nbody {\n    display: grid;\n    grid-template-columns: repeat(auto-fill, minmax(100px, 1fr));\n    grid-gap: 10px;\n    padding: 10px;\n    font-size: 10px;\n    min-height: 100vh;\n\n    @include desktop {\n        grid-template-columns: repeat(18, 1fr);\n        grid-template-rows: repeat(7, 1fr);\n    }\n}\n\n.el {\n    background: lightgray;\n    height: 150px;\n\n    @include desktop {\n        height: 100px;\n    }\n\n    @for $i from 1 through length($types) {\n        $type: nth($types, $i);\n        $index: index($types, $type);\n        $color: hsl($index* ((360deg/length($types))) 45% 65%);\n\n\n        &--#{$type} {\n            background: $color;\n        }\n    }\n\n}\n\n@for $a from 1 through 7 {\n\n    article[data-period=\"#{$a}\"] {\n        grid-row: #{$a};\n    }\n\n}\n\n@for $a from 1 through 18 {\n\n    article[data-group=\"#{$a}\"] {\n        grid-column: #{$a};\n    }\n\n}"},"lineCount":null}},"error":null,"hash":"10e6ae163f2cd1c4387203179d3277f8","cacheData":{"env":{}}}